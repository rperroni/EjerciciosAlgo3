!MayorAUno methodsFor: 'arithmetic operations' stamp: 'RP 10/25/2021 10:24:26'!
/ aDivisor

	| greatestCommonDivisor numerator denominator |
	
	greatestCommonDivisor := self greatestCommonDivisorWith: aDivisor.
	numerator := self // greatestCommonDivisor .
	denominator := aDivisor // greatestCommonDivisor .
	
	^denominator divideEntero: numerator! !

!MayorAUno methodsFor: 'arithmetic operations' stamp: 'Sebas 10/25/2021 08:38:59'!
