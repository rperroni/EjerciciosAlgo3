	poolDictionaries: ''
	category: 'Numeros-Parte2-Ejercicio'!

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:32:22'!
* aMultiplier 
		
	^ aMultiplier beMultipliedByFraccion: self.
	
	! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:32:34'!
+ anAdder 	
		
	^anAdder beAddedToFraccion: self.! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:32:41'!
- aSubtrahend 
		
	^aSubtrahend beSubtractedFromFraccion: self.
! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:32:48'!
/ aDivisor 
	
	^aDivisor divideFraccion: self.! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'Sebas 10/22/2021 11:40:33'!
beAddedToEntero: anAugendEntero

	^((anAugendEntero * self denominator) + self numerator) / self denominator.! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'Sebas 10/22/2021 11:42:29'!
beAddedToFraccion: anAugendFraccion
	
	| newNumerator newDenominator |
	newNumerator := (numerator * anAugendFraccion denominator) + (denominator * anAugendFraccion numerator).
	newDenominator := denominator * anAugendFraccion denominator.
	
	^newNumerator / newDenominator! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'Sebas 10/22/2021 11:46:29'!
beMultipliedByEntero: aMultiplicandEntero

	^(aMultiplicandEntero * numerator) / denominator.	! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'Sebas 10/22/2021 11:47:13'!
beMultipliedByFraccion: aMultiplicandFraccion

	^(numerator * aMultiplicandFraccion numerator) / (denominator * aMultiplicandFraccion denominator).! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:33:02'!
beSubtractedFromEntero: aMinuendEntero
	
	^( aMinuendEntero * denominator - numerator ) / denominator.! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:33:11'!
beSubtractedFromFraccion: aMinuendFraccion
	
	^((aMinuendFraccion numerator * denominator) - (aMinuendFraccion denominator * numerator)) / (aMinuendFraccion denominator * denominator).
! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:33:17'!
divideEntero: aDividendEntero
		
	^(denominator * aDividendEntero) / numerator.! !

!Fraccion methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:33:25'!
divideFraccion: aDividendFraccion
	
