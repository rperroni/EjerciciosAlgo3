	^Fraccion new initializeWith:  aDividendEntero over:   self.
	
	! !

!MayorAUno methodsFor: 'arithmetic operations' stamp: 'RP 10/23/2021 16:42:59'!
fibonacci

	| one two |
	
	one := Entero with: 1.
	two := Entero with: 2.
	
	^ (self - one) fibonacci + (self - two) fibonacci! !

